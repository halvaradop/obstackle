{
  "name": "@halvaradop/obstackle",
  "version": "0.2.0",
  "description": "A collection of utility functions for working with objects in TypeScript.",
  "type": "module",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halvaradop/obstackle.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "objects",
    "utilities",
    "tools",
    "toolkit",
    "functions",
    "manipulation"
  ],
  "author": "Hernan Alvarado <hernanvid123@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/halvaradop/obstackle",
  "bugs": {
    "email": "hernanvid123@gmail.com",
    "url": "https://github.com/halvaradop/obstackle/issues"
  },
  "dependencies": {
    "@halvaradop/obstackle": "link:",
    "@halvaradop/ts-utility-types": "^0.20.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "prettier": "^3.5.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.6"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./clone": {
      "types": "./dist/clone.d.ts",
      "require": "./dist/clone.cjs",
      "import": "./dist/clone.js"
    },
    "./deep": {
      "types": "./dist/deep.d.ts",
      "require": "./dist/deep.cjs",
      "import": "./dist/deep.js"
    },
    "./get": {
      "types": "./dist/get.d.ts",
      "require": "./dist/get.cjs",
      "import": "./dist/get.js"
    },
    "./has": {
      "types": "./dist/has.d.ts",
      "require": "./dist/has.cjs",
      "import": "./dist/has.js"
    },
    "./omit": {
      "types": "./dist/omit.d.ts",
      "require": "./dist/omit.cjs",
      "import": "./dist/omit.js"
    },
    "./pick": {
      "types": "./dist/pick.d.ts",
      "require": "./dist/pick.cjs",
      "import": "./dist/pick.js"
    },
    "./set": {
      "types": "./dist/set.d.ts",
      "require": "./dist/set.cjs",
      "import": "./dist/set.js"
    }
  },
  "packageManager": "pnpm@10.3.0",
  "prettier": {
    "printWidth": 130,
    "tabWidth": 4,
    "semi": false,
    "overrides": [
      {
        "files": [
          "*.json",
          "*.md",
          "*.yml",
          "*.yaml"
        ],
        "options": {
          "tabWidth": 2
        }
      }
    ]
  }
}
