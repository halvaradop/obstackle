{
  "name": "@halvaradop/obstackle",
  "version": "0.1.0",
  "description": "A collection of utility functions for working with objects in TypeScript.",
  "type": "module",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halvaradop/obstackle.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "objects",
    "utilities",
    "tools",
    "toolkit",
    "functions",
    "manipulation"
  ],
  "author": "Hernan Alvarado <hernanvid123@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/halvaradop/obstackle",
  "bugs": {
    "email": "hernanvid123@gmail.com",
    "url": "https://github.com/halvaradop/obstackle/issues"
  },
  "devDependencies": {
    "@halvaradop/ts-utility-types": "^0.19.0",
    "@types/node": "^22.13.5",
    "prettier": "^3.5.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.6"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./deep": {
      "types": "./dist/deep.d.ts",
      "require": "./dist/deep.cjs",
      "import": "./dist/deep.js"
    }
  },
  "packageManager": "pnpm@10.3.0",
  "prettier": {
    "printWidth": 130,
    "tabWidth": 4,
    "semi": false,
    "overrides": [
      {
        "files": [
          "*.json",
          "*.md",
          "*.yml",
          "*.yaml"
        ],
        "options": {
          "tabWidth": 2
        }
      }
    ]
  }
}
